# App Environment
NEXT_PUBLIC_APP_ENV=staging

# API Configuration
NEXT_PUBLIC_API_URL=https://api-staging.mindmate.com
NEXT_PUBLIC_API_TIMEOUT=30000

# Feature Flags
NEXT_PUBLIC_ENABLE_REFLECTIONS=true
NEXT_PUBLIC_ENABLE_FEEDBACK=true
NEXT_PUBLIC_ENABLE_ANALYTICS=true

# Analytics
NEXT_PUBLIC_ANALYTICS_ID=UA-XXXXXXXXX-2

# Error Tracking
NEXT_PUBLIC_SENTRY_DSN=https://xxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxx@sentry.io/XXXXXX

# Performance Monitoring
NEXT_PUBLIC_ENABLE_PERFORMANCE_MONITORING=true

# Build Optimization
NEXT_PUBLIC_ENABLE_COMPRESSION=true
NEXT_PUBLIC_ENABLE_CACHING=true

# Security Headers
NEXT_PUBLIC_CSP_REPORT_ONLY=true
NEXT_PUBLIC_HSTS_ENABLED=true

# Note: Replace placeholder values (XXXXXX) with actual staging environment values
# These values should be set in Vercel dashboard for security 