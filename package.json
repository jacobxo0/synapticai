{
  "name": "synapticai",
  "version": "0.1.0",
  "private": true,
  "packageManager": "yarn@4.1.0",
  "scripts": {
    "dev": "yarn prisma generate && next dev",
    "build": "yarn prisma generate && next build",
    "postbuild": "ts-node scripts/verify-build.ts",
    "verify": "ts-node scripts/verify-build.ts",
    "start": "next start",
    "lint": "next lint",
    "typecheck": "tsc --noEmit",
    "prisma:generate": "prisma generate",
    "prisma:push": "prisma db push",
    "prisma:seed": "ts-node prisma/seed.ts",
    "postinstall": "prisma generate",
    "db:push": "prisma db push",
    "db:seed": "prisma db seed",
    "prisma:studio": "prisma studio",
    "clean": "rm -rf prisma/generated/",
    "prepare": "yarn install"
  },
  "prisma": {
    "seed": "ts-node --compiler-options {\"module\":\"CommonJS\"} prisma/seed.ts"
  },
  "dependencies": {
    "@auth/prisma-adapter": "^2.9.0",
    "@prisma/client": "^6.6.0",
    "@upstash/redis": "^1.34.8",
    "chalk": "^4.1.2",
    "dayjs": "^1.11.13",
    "framer-motion": "^12.7.4",
    "next": "14.1.0",
    "next-auth": "^5.0.0-beta.26",
    "prisma": "^6.6.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "recharts": "^2.15.2",
    "tailwindcss": "^3.4.17",
    "zod": "^3.24.3",
    "zustand": "^4.5.6"
  },
  "devDependencies": {
    "@tailwindcss/forms": "^0.5.7",
    "@tailwindcss/typography": "^0.5.10",
    "@types/marked": "^5.0.2",
    "@types/node": "^20.11.0",
    "@types/puppeteer": "^5.4.7",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@types/uuid": "^10.0.0",
    "@vitejs/plugin-react": "^4.4.1",
    "autoprefixer": "^10.4.21",
    "eslint": "^8.56.0",
    "eslint-config-next": "14.1.0",
    "postcss": "^8.4.35",
    "postcss-import": "^16.1.0",
    "postcss-nesting": "^13.0.1",
    "tailwind-merge": "^3.2.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3",
    "vitest": "^3.1.2"
  }
}
